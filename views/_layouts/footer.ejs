        <script type="text/javascript">
            $('body').click(function() {
                $('#searchResult').hide();
            });
            $('#search').keyup(function() {
                if ($(this).val().length == 0) {
                    $("#searchResult").empty();
                }
            });
            $('#search').autocomplete({
                source: function (req, res) {
                    if (req.term.trim().length == 0) {
                        $("#searchResult").empty();
                    } else {
                        $.ajax({
                            url: "http://localhost:3000/autocomplete?term="+req.term.trim(),
                            // dataType: "jsonp",
                            type: "GET",
                            success: function (data) {
                                // console.log(data);
                                $("#searchResult").empty();
                                $('#searchResult').show();
                                for (let i = 0; i < data.length; i++) {
                                    $("#searchResult").append("<li value='"+data[i]._id+"'>"+data[i].productname+"</li>");                                
                                }
                            },
                            error: function (xhr) {
                                alert(xhr.status + ' : ' + xhr.statusText);
                            }
                        });
                    }
                }    
            });
        </script>
