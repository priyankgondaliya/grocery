    <!-- autocomplete search -->
    <script type="text/javascript">
        $( document ).ready(function() {
            $('#searchResult').hide();
        });
        $(document).mouseup(function(e) 
        {
            var container = $("#search_div");
            // if the target of the click isn't the container nor a descendant of the container
            if (!container.is(e.target) && container.has(e.target).length === 0) 
            {
                $('#searchResult').hide();
            }
        });
        $('#search').keyup(function() {
            if ($(this).val().length == 0) {
                $("#searchResult").hide();
            }
        });
        $('#search').focus(function() {
            if ( $('#searchResult').children().length > 0 ) {
                $("#searchResult").show();
            }
        });
        $('#search').autocomplete({
            source: function (req, res) {
                if (req.term.replace('/','').replace('\\','').trim().length == 0) {
                    $("#searchResult").empty();
                } else {
                    $.ajax({
                        url: "/autocomplete?term="+req.term.trim(),
                        // dataType: "jsonp",
                        type: "GET",
                        success: function (data) {
                            // console.log(data);
                            $("#searchResult").empty();
                            $('#searchResult').show();
                            if (data.length == 0) {
                                $("#searchResult").append(`<li>No Products Found!</li>`);
                            }
                            for (let i = 0; i < data.length; i++) {
                                $("#searchResult").append(`<li><a href="/products/detail/${data[i]._id}">${data[i].productname}</a></li>`);
                            }
                        },
                        error: function (xhr) {
                            alert(xhr.status + ' : ' + xhr.statusText);
                        }
                    });
                }
            }    
        });
    </script>
